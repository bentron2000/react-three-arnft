{
  "name": "react-three-arnft",
  "version": "0.3.12",
  "description": "Fork of Image tracking with @kalwalt/jsartoolkit-nft and @react-three/fiber",
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "watch": "babel --watch src --out-dir lib",
    "build": "babel src --out-dir lib --source-maps && mkdir -p lib/arnft/worker && cp -R node_modules/@webarkit/jsartoolkit-nft/dist/* lib/arnft/worker/"
  },
  "engines": {
    "node": ">= 16.15.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bentron2000/react-three-arnft.git"
  },
  "keywords": [
    "react",
    "react-three-fiber",
    "jsartoolkit-nft",
    "arnft",
    "imagetracking",
    "ar",
    "three"
  ],
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/bentron2000/react-three-arnft/issues"
  },
  "homepage": "https://github.com/bentron2000/react-three-arnft#readme",
  "peerDependencies": {
    "@react-three/fiber": "^8.8.10",
    "@webarkit/jsartoolkit-nft": "^1.1.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@react-three/fiber": "9.0.0-rc.1",
    "core-js": "^3.25.5",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "2.7.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
